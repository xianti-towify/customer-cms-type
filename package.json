  "version": "0.0.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "tsc": "tsc --build --force",
    "pre-commit": "lint-staged",
    "prepare": "husky install",
    "lint": "eslint --ext .js, .jsx, .ts, .tsx src",
